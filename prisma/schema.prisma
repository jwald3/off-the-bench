generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model player_defense_stats_basic {
  player_id            String?
  game_id              String?
  down                 BigInt?
  passes_defended      Float?
  interception         Float?
  int_return_yards     Float?
  int_return_touchdown Float?
  tackles_for_loss     Float?
  qb_hits              Float?
  fumbles_forced       Float?
  solo_tackles         Float?
  assist_tackls        Float?
  sack                 Float?
  half_sack            Float?
  tackle_with_assist   Float?
  game_id_db           String  @id
  gsis_id              String?
  team_abbr            String?
  position             String?
  week                 Int?
  season               Int?
  week_count           BigInt?
}

model player_offense_stats_basic {
  player_id        String?
  game_id          String?
  down             BigInt?
  season           Int?
  pass_attempt     Float?
  completion       Float?
  incompletion     Float?
  passing_yards    Float?
  passing_TD       Float?
  interception     Float?
  sack             Float?
  rush_attempt     Float?
  rushing_yards    Float?
  rushing_TD       Float?
  tackled_for_loss Float?
  fumble           Float?
  reception        Float?
  target           Float?
  receiving_yards  Float?
  receiving_TD     Float?
  game_id_db       String  @id
  gsis_id          String?
  team_abbr        String?
  position         String?
  week             Int?
  week_count       BigInt?
}

model team_defense_stats_basic {
  season         BigInt?
  posteam        String?
  game_id        String?
  down           BigInt?
  points_for     Float?
  points_allowed Int?
  completions    Float?  @db.Real
  pass_attempts  Float?  @db.Real
  interceptions  Float?  @db.Real
  sacks          Float?  @db.Real
  passing_yards  Float?  @db.Real
  pass_touchdown Float?  @db.Real
  rush_attempt   Float?  @db.Real
  rushing_yards  Float?  @db.Real
  rush_touchdown Float?  @db.Real
  fumbles        Float?  @db.Real
  week           Int?
  db_id          String  @id
}

model team_offense_stats_basic {
  season         BigInt?
  posteam        String?
  game_id        String?
  down           BigInt?
  points_for     Int?
  points_allowed Float?
  completions    Float?  @db.Real
  pass_attempts  Float?  @db.Real
  interceptions  Float?  @db.Real
  sacks          Float?  @db.Real
  passing_yards  Float?  @db.Real
  pass_touchdown Float?  @db.Real
  rush_attempt   Float?  @db.Real
  rushing_yards  Float?  @db.Real
  rush_touchdown Float?  @db.Real
  fumbles        Float?  @db.Real
  week           Int?
  db_id          String  @id
}

model player_usage_by_team {
  posteam                 String?
  game_id                 String?
  down                    BigInt?
  player_id               String?
  position                String?
  targets                 BigInt?
  receptions              BigInt?
  receiving_yards         BigInt?
  air_yards               BigInt?
  yards_after_catch       BigInt?
  receiving_touchdown     BigInt?
  redzone_target          BigInt?
  redzone_catch           BigInt?
  endzone_target          BigInt?
  endzone_catch           BigInt?
  first_down_target       BigInt?
  second_down_target      BigInt?
  third_down_target       BigInt?
  fourth_down_target      BigInt?
  first_down_catch        BigInt?
  second_down_catch       BigInt?
  third_down_catch        BigInt?
  fourth_down_catch       BigInt?
  rush_attempt            BigInt?
  rushing_yards           BigInt?
  rush_touchdown          BigInt?
  stacked_box_rush        BigInt?
  tackled_for_loss        BigInt?
  fumble                  BigInt?
  first_down_rush         BigInt?
  second_down_rush        BigInt?
  third_down_rush         BigInt?
  fourth_down_rush        BigInt?
  redzone_rush            BigInt?
  goalline_rush           BigInt?
  redzone_rush_touchdown  BigInt?
  goalline_rush_touchdown BigInt?
  gsis_id                 String?
  db_id                   String  @id
  season                  Int?
  week                    Int?
  week_count              BigInt?
}

model team_personnel_data {
  game_id          String?
  posteam          String?
  down             BigInt?
  offense_grouping String?
  snap_ct          BigInt?
  passing_snap     BigInt?
  rushing_snap     BigInt?
  passing_yards    BigInt?
  pass_touchdown   BigInt?
  rushing_yards    BigInt?
  rush_touchdown   BigInt?
  season           Int?
  week             Int?
  week_count       BigInt?
  db_id            String  @id
  total_game_snaps BigInt?
}

model game_logs_basic {
  game_id               String?
  posteam               String?
  defteam               String?
  posteam_score         Float?  @db.Real
  defteam_score         Float?  @db.Real
  yards_gained          Float?  @db.Real
  turnovers             Int?
  fumbles               Float?  @db.Real
  fumbles_lost          Float?  @db.Real
  completed_passes      Float?  @db.Real
  incomplete_passes     Float?  @db.Real
  pass_attempts         Float?  @db.Real
  receiving_yards       Float?  @db.Real
  passing_touchdowns    Float?  @db.Real
  interceptions         Float?  @db.Real
  sacks                 Float?  @db.Real
  rush_attempts         Float?  @db.Real
  rushing_yards         Float?  @db.Real
  rushing_touchdown     Float?  @db.Real
  extra_points_made     Int?
  extra_point_attempts  Float?  @db.Real
  field_goals_made      Int?
  field_goals_attempted Float?  @db.Real
  time_of_possession    String?
  penalties             Float?  @db.Real
  penalty_yards         Float?  @db.Real
  db_id                 String  @id
  season                Int?
  week                  Int?
}

model down_by_down_offense {
  game_id        String?
  posteam        String?
  down           Int?
  special        Float?
  rush_attempt   Float?  @db.Real
  rushing_yards  Float?  @db.Real
  rush_touchdown Float?  @db.Real
  complete_pass  Float?  @db.Real
  pass_attempt   Float?  @db.Real
  passing_yards  Float?  @db.Real
  pass_touchdown Float?  @db.Real
  interception   Float?  @db.Real
  sack           Float?  @db.Real
  season         Int?
  week           Int?
  db_id          String  @id
}

model team_information {
  team_abbr            String  @id
  team_name            String?
  team_id              BigInt?
  team_nick            String?
  team_conf            String?
  team_division        String?
  wins                 BigInt?
  losses               BigInt?
  ties                 BigInt?
  standing             String?
  team_color           String?
  team_color2          String?
  team_color3          String?
  team_color4          String?
  team_logo_wikipedia  String?
  team_logo_espn       String?
  team_wordmark        String?
  team_conference_logo String?
  team_league_logo     String?
  team_logo_squared    String?
}

model conversion_success {
  posteam               String?
  game_id               String?
  play_type             String?
  third_down_converted  Int?
  third_down_failed     Int?
  fourth_down_converted Int?
  fourth_down_failed    Int?
  third_down_attempt    Int?
  fourth_down_attempt   Int?
  week                  Int?
  season                Int?
  db_id                 String  @id
}

model offense_player_snaps_by_down {
  player_id           String?
  game_id             String?
  posteam             String?
  down                Int?
  snap_ct             BigInt?
  rush_snap           Float?
  pass_snap           Float?
  team_snaps          Float?
  team_rushing_plays  Float?
  team_passing_plays  Float?
  week                Int?
  season              Int?
  target              Float?
  reception           Float?
  receiving_touchdown Float?
  carries             Float?
  rushing_touchdown   Float?
  db_id               String  @id
  gsis_id             String?
}

model defense_player_snaps_by_down {
  player_id          String?
  game_id            String?
  defteam            String?
  key                BigInt?
  down               Float?
  snap_ct            BigInt?
  rush_snap          Float?
  pass_snap          Float?
  team_snaps         Float?
  team_rushing_plays Float?
  team_passing_plays Float?
  week               Int?
  season             Int?
  gsis_id            String?
  db_id              String  @id
}

model advanced_receiving_stats {
  player_id              String?
  game_id                String?
  posteam                String?
  key                    BigInt?
  down                   Int?
  passing_yards          Float?
  air_yards              Float?
  yards_after_catch      Float?
  complete_pass          Float?
  pass_attempt           Float?
  interception           Float?
  pass_touchdown         Float?
  first_down_pass        Float?
  red_zone_target        Float?
  red_zone_rec           Float?
  red_zone_yards         Float?
  end_zone_target        Float?
  end_zone_rec           Float?
  team_passing_yards     Float?
  team_air_yards         Float?
  team_yards_after_catch Float?
  team_complete_pass     Float?
  team_pass_attempt      Float?
  team_interception      Float?
  team_pass_touchdown    Float?
  team_first_down_pass   Float?
  team_red_zone_target   Float?
  team_red_zone_rec      Float?
  team_red_zone_yards    Float?
  team_end_zone_target   Float?
  team_end_zone_rec      Float?
  gsis_id                String?
  week                   Int?
  season                 Int?
  db_id                  String  @id
}
